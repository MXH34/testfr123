local WindUI = loadstring(game:HttpGet("https://github.com/Footagesus/WindUI/releases/latest/download/main.lua"))()

local Window = WindUI:CreateWindow({
    Title = "BetterRingta",
    Icon = "star",
    Author = "Ringta",
    Folder = "Ringtrash"
})

-- blacklist check
local blacklisted = {["swift"]=true,["volcano"]=true,["potassium"]=true,["bunni"]=true}

function identifyexecutor()
    local probes = {}
    local ok, res
    if type(getexecutor) == "function" then ok, res = pcall(getexecutor) if ok and res then table.insert(probes, tostring(res)) end end
    if type(getexploit) == "function" then ok, res = pcall(getexploit) if ok and res then table.insert(probes, tostring(res)) end end
    for k, v in pairs(_G) do
        table.insert(probes, tostring(k))
        if type(v) == "string" then table.insert(probes, tostring(v)) end
    end
    for _, p in ipairs(probes) do
        local s = tostring(p):lower()
        for name, _ in pairs(blacklisted) do
            if string.find(s, name, 1, true) then
                pcall(function() game.Players.LocalPlayer:Kick("Blacklisted Executor") end)
                return name
            end
        end
    end
    return nil
end

identifyexecutor()

-- customize the WindUI open button
do
    local btn = Window.OpenButton
    if btn then
        btn.Text = "Open RINGTA SCRIPTS"
        btn.Size = UDim2.new(0, 220, 0, 40)
        btn.TextScaled = true
        btn.AutoButtonColor = true

        local corner = Instance.new("UICorner")
        corner.CornerRadius = UDim.new(0, 8)
        corner.Parent = btn

        local stroke = Instance.new("UIStroke")
        stroke.Thickness = 2
        stroke.ApplyStrokeMode = Enum.ApplyStrokeMode.Border
        stroke.Color = Color3.fromRGB(255, 255, 255)
        stroke.Parent = btn

        local gradient = Instance.new("UIGradient")
        gradient.Color = ColorSequence.new{
            ColorSequenceKeypoint.new(0, Color3.fromRGB(255, 0, 255)),
            ColorSequenceKeypoint.new(1, Color3.fromRGB(0, 255, 255))
        }
        gradient.Parent = stroke
    end
end

-- example tab + section + button
local Tab = Window:CreateTab({
    Title = "Main",
    Icon = "home"
})

local Section = Tab:CreateSection("General")

Section:CreateButton({
    Title = "Test Button",
    Callback = function()
        print("clicked")
    end
})
